#!/bin/bash
#
# W. H. Bell
#
# A script to create desktop icons that link to each of the BrickPi Scratch
# programs.
#

if [[ $# != 1 ]]; then
  echo "Usage $0 <example directory>"
fi
program_files=""


[Desktop Entry]
Encoding=UTF-8
Type=Application
Name=Tank
Comment=Tank
Icon=scratch
Exec=lxterminal -e /home/pi/bin/tank -t RpiScratchIO
Terminal=false
